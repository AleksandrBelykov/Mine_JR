public class forCycle {

	public static void main(String[] args) {
		int[][] multiplitable = new int [10][10];
		
		for (int i = 0; i < 10; i++) {
			for (int j = 0; j < 10; j++) {
                multiplitable[i][j]=(i+1)*(j+1);
                System.out.print(multiplitable[i][j]+" ");
            }
			System.out.println();
        }}}
