package Level_4;
import java.io.BufferedReader;
import java.io.InputStreamReader;
/* 
Настя или Настя?

Ввести с клавиатуры два имени, и
если имена одинаковые вывести сообщение "Имена идентичны".
Если имена разные, но их длины равны - вывести сообщение - "Длины имен равны".
Если имена и длины имен разные - ничего не выводить.
*/


public class Task_0421 {
	
	public static void main(String[] args) throws Exception {
        BufferedReader buff = new BufferedReader(new InputStreamReader(System.in));
        String name1str = buff.readLine();
        String name2str = buff.readLine();
		
        char[] name1char= name1str.toCharArray();
        char[] name2char= name2str.toCharArray();
        /*System.out.println(name1char.length);
        System.out.println(name2char.length);*/
        		      
        	if (name1str.equals(name2str))
			System.out.println("Имена идентичны");
		
			
			else {
				if (name1char.length==name2char.length)
				System.out.println("Длины имен равны");}
			
	}

}
